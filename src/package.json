{
  "name": "aws-lambda-response",
  "version": "1.0.3",
  "description": "A simple package for sending standard responses in AWS Lambda callback functions.",
  "main": "index.js",
  "scripts": {
    "test": "npm run-script lint && npm run-script unit",
    "lint": "eslint {./*.js\\,./tests/*.js}",
    "unit": "babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/mocha/bin/_mocha -- tests/*.js",
    "compile": "babel . --out-dir ../dist --ignore='node_modules/**\\,coverage/**' --copy-files",
    "deploy": "cp ../README.md README.md && npm publish"
  },
  "keywords": [
    "response",
    "structure",
    "lambda",
    "api",
    "gateway"
  ],
  "author": {
    "name": "Aaron Williams",
    "email": "aaronwilliams9797@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aceew/aws-lambda-response.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
